for(const event of events){const start=moment(event.extendedProps.r_start);const end=moment(event.extendedProps.r_end);const duration=moment.duration(end.diff(start)).asDays();const record=records[event.extendedProps.record.id];event.extendedProps.record.start_hour=!record.allDay&&duration<1?start.format("HH:mm"):"";const key=this._getFormattedDate(start,end,false,record.allDay);if(!(key in groupedEvents)){groupedEvents[key]=[];groupKeys.push({key:key,start:event.start,end:event.end,isSameDayEvent:start.clone().add(1,'minute').isSame(end.clone().subtract(1,'minute'),'day'),});}
