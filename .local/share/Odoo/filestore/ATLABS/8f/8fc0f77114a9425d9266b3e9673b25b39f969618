this.$buttons.on('click','.o_button_at_date',this._onOpenWizard.bind(this));},_onOpenWizard:function(){var state=this.model.get(this.handle,{raw:true});var stateContext=state.getContext();var context={active_model:this.modelName,};if(stateContext.default_product_id){context.product_id=stateContext.default_product_id;}else if(stateContext.product_tmpl_id){context.product_tmpl_id=stateContext.product_tmpl_id;}
