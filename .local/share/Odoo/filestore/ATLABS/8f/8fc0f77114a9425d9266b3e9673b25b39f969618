odoo.define('web.GroupByMenu',function(require){"use strict";const CustomGroupByItem=require('web.CustomGroupByItem');const{FACET_ICONS,GROUPABLE_TYPES}=require('web.searchUtils');const{useModel}=require('web.Model');const{SearchDropdownItem}=require("@web/search/search_dropdown_item/search_dropdown_item");const{Component}=owl;class GroupByMenu extends Component{constructor(){super(...arguments);this.icon=FACET_ICONS.groupBy;this.fields=Object.values(this.props.fields).filter(field=>this._validateField(field)).sort(({string:a},{string:b})=>a>b?1:a<b?-1:0);this.model=useModel('searchModel');}
